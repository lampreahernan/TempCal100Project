<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="200.0" prefWidth="525.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.60" fx:controller="com.co.tempcal.vista.DatosTemperaturaController">
   <children>
      <Label layoutX="14.0" layoutY="28.0" text="Please put the Soak Sensor in the Cold Bath and wait while the temperature is stabilized" textAlignment="CENTER" textOverrun="CLIP">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <HBox layoutX="125.0" layoutY="88.0">
         <children>
            <Label text="Cold Bath Temperature " />
            <TextField fx:id="txtColdBathTemp" />
         </children>
      </HBox>
      <Button fx:id="btnSiguiente" layoutX="372.0" layoutY="151.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="102.0" text="Next" onAction="#handleSiguiente"/>
   </children>
</AnchorPane>
