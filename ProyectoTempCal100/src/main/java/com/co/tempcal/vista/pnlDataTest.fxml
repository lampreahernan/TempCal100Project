<AnchorPane prefHeight="250.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.co.tempcal.vista.TemperatureInfoController">
   <children>
      <GridPane layoutX="67.0" layoutY="65.0" prefHeight="60.0" prefWidth="267.0">
        <columnConstraints>
